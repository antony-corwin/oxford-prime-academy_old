{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "oxford-prime-academy": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/oxford-prime-academy",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              {
                "input": "./src/app/@core/services/primeng-theme/themes/arya-blue.scss",
                "bundleName": "arya-blue",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/arya-green.scss",
                "bundleName": "arya-green",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/arya-orange.scss",
                "bundleName": "arya-orange",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/arya-purple.scss",
                "bundleName": "arya-purple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/bootstrap4-dark-blue.scss",
                "bundleName": "bootstrap4-dark-blue",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/bootstrap4-dark-purple.scss",
                "bundleName": "bootstrap4-dark-purple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/bootstrap4-light-blue.scss",
                "bundleName": "bootstrap4-light-blue",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/bootstrap4-light-purple.scss",
                "bundleName": "bootstrap4-light-purple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/fluent-light.scss",
                "bundleName": "fluent-light",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/luna-amber.scss",
                "bundleName": "luna-amber",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/luna-blue.scss",
                "bundleName": "luna-blue",
                "inject": false
              },              {
                "input": "./src/app/@core/services/primeng-theme/themes/luna-green.scss",
                "bundleName": "luna-green",
                "inject": false
              },              {
                "input": "./src/app/@core/services/primeng-theme/themes/luna-pink.scss",
                "bundleName": "luna-pink",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/md-dark-deeppurple.scss",
                "bundleName": "md-dark-deeppurple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/md-dark-indigo.scss",
                "bundleName": "md-dark-indigo",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/md-light-deeppurple.scss",
                "bundleName": "md-light-deeppurple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/md-light-indigo.scss",
                "bundleName": "md-light-indigo",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/mdc-dark-deeppurple.scss",
                "bundleName": "mdc-dark-deeppurple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/mdc-dark-indigo.scss",
                "bundleName": "mdc-dark-indigo",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/mdc-light-deeppurple.scss",
                "bundleName": "mdc-light-deeppurple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/mdc-light-indigo.scss",
                "bundleName": "mdc-light-indigo",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/nova.scss",
                "bundleName": "nova",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/nova-accent.scss",
                "bundleName": "nova-accent",
                "inject": false
              },              {
                "input": "./src/app/@core/services/primeng-theme/themes/nova-alt.scss",
                "bundleName": "nova-alt",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/rhea.scss",
                "bundleName": "rhea",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/saga-blue.scss",
                "bundleName": "saga-blue",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/saga-green.scss",
                "bundleName": "saga-green",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/saga-orange.scss",
                "bundleName": "saga-orange",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/saga-purple.scss",
                "bundleName": "saga-purple",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/vela-blue.scss",
                "bundleName": "vela-blue",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/vela-green.scss",
                "bundleName": "vela-green",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/vela-orange.scss",
                "bundleName": "vela-orange",
                "inject": false
              },
              {
                "input": "./src/app/@core/services/primeng-theme/themes/vela-purple.scss",
                "bundleName": "vela-purple",
                "inject": false
              },
              "src/theme/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "test:build:production"
            },
            "development": {
              "browserTarget": "oxford-prime-academy:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "e2e": {
          "builder": "@briebug/cypress-schematic:cypress",
          "options": {
            "devServerTarget": "oxford-prime-academy:serve",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "oxford-prime-academy:serve:production"
            }
          }
        },
        "cypress-run": {
          "builder": "@briebug/cypress-schematic:cypress",
          "options": {
            "devServerTarget": "oxford-prime-academy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "oxford-prime-academy:serve:production"
            }
          }
        },
        "cypress-open": {
          "builder": "@briebug/cypress-schematic:cypress",
          "options": {
            "devServerTarget": "oxford-prime-academy:serve",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "oxford-prime-academy:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "oxford-prime-academy"
}
